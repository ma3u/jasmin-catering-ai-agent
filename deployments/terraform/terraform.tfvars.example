# Azure Configuration
azure_subscription_id = "your-subscription-id-here"
resource_group_name   = "logicapp-jasmin-sweden_group"
location             = "swedencentral"  # Required: West Europe not accepting new customers
environment          = "production"

# Logic App Configuration
logic_app_name = "jasmin-order-processor-sweden"

# AI Services Configuration
ai_services_endpoint = "https://jasmin-catering-resource.cognitiveservices.azure.com"
ai_api_key          = "your-ai-api-key-here"
ai_model_deployment = "gpt-4o"
ai_api_version      = "2024-02-01"
ai_temperature      = 0.3
ai_max_tokens       = 1500

# Email Configuration
webde_email_alias   = "ma3u-test@email.de"
webde_app_password  = "your-webde-app-password-here"

# Business Configuration
business_name        = "Jasmin Catering"
business_location    = "Berlin, Germany"
min_guests          = 15
max_guests          = 500
price_per_person_min = 35
price_per_person_max = 45

# Monitoring Configuration
email_check_interval = 5
email_retention_days = 30